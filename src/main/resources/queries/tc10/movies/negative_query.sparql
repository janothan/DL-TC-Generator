PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX dbr: <http://dbpedia.org/resource/>

SELECT DISTINCT ?x WHERE
{
    {
        ?x a dbo:Film .
    }
    FILTER
    (
        NOT EXISTS
        {
            ?y1 dbo:lastAppearance ?x .
            ?y2 dbo:lastAppearance ?x .
            FILTER
            (
                ?y1 != ?y2
            ) .
        }
    )
} LIMIT <number>