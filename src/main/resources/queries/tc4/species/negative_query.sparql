PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX dbr: <http://dbpedia.org/resource/>

SELECT DISTINCT ?x WHERE
{

    FILTER
    (
        NOT EXISTS
        {
            ?x ?y dbr:Animal .
        }
        &&
        NOT EXISTS
        {
            dbr:Animal ?z ?x .
        }
    ) .
} LIMIT <number>