PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX dbr: <http://dbpedia.org/resource/>

PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX dbr: <http://dbpedia.org/resource/>

SELECT DISTINCT ?x WHERE
{
    {
        ?x a dbo:BasketballPlayer .
    }
  	FILTER
  	(
    	NOT EXISTS
    	{
      		?x dbo:team ?y .
        	?y a dbo:BasketballTeam .
    	}
  	)
} LIMIT <number>