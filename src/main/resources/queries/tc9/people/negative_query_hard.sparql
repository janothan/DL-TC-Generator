PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX dbr: <http://dbpedia.org/resource/>

SELECT DISTINCT ?x WHERE
{
    {
        ?x a dbo:Person .
        ?x dbo:award ?y .
    }
    FILTER
    (
        NOT EXISTS
        {
            ?x dbo:award ?z .
            FILTER
            (
                ?y != ?z
            )
        }
    ) .

} LIMIT <number>